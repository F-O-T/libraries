{
   "name": "@f-o-t/spelling",
   "version": "1.0.1",
   "description": "Hunspell-compatible spell checking library with Zod schemas",
   "type": "module",
   "private": false,
   "module": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "exports": {
      ".": {
         "bun": "./src/index.ts",
         "import": {
            "default": "./dist/index.js",
            "types": "./dist/index.d.ts"
         },
         "types": "./src/index.ts"
      },
      "./plugins/browser-storage": {
         "bun": "./src/plugins/browser-storage/index.ts",
         "import": {
            "default": "./dist/plugins/browser-storage/index.js",
            "types": "./dist/plugins/browser-storage/index.d.ts"
         },
         "types": "./src/plugins/browser-storage/index.ts"
      },
      "./package.json": "./package.json"
   },
   "files": [
      "dist"
   ],
   "scripts": {
      "build": "fot build",
      "check": "fot check",
      "format": "fot format",
      "lint": "fot lint",
      "test": "fot test"
   },
   "dependencies": {
      "zod": "^3.24.1"
   },
   "devDependencies": {
      "@biomejs/biome": "2.3.10",
      "@f-o-t/cli": "workspace:*",
      "@f-o-t/config": "workspace:*",
      "@types/bun": "1.3.5",
      "typescript": "5.9.3"
   },
   "peerDependencies": {
      "typescript": ">=4.5.0"
   },
   "peerDependenciesMeta": {
      "typescript": {
         "optional": true
      }
   },
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/F-O-T/contentta-nx.git"
   },
   "homepage": "https://github.com/F-O-T/contentta-nx/blob/master/libraries/spelling",
   "bugs": {
      "url": "https://github.com/F-O-T/contentta-nx/issues"
   },
   "publishConfig": {
      "access": "public"
   }
}
