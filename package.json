         },
         "vite": {
            "@tailwindcss/vite": "^4.1.8",
            "@vitejs/plugin-react": "^4.3.4",
            "vite": "7.2.4",
            "vite-tsconfig-paths": "^5.1.4"
         },
         "workers": {
            "bullmq": "5.58.7",
